 <!-- Modal Structure -->
 <div id="modalclients" class="modal modal-fixed-footer">
    <div class="modal-content">
      <h4>Comanda</h4>
      <p>Selecione a Comanda</p>

      <div class="input-field col s6 m4">
          <input placeholder="Comanda" id="clientinput" type="number" class="validate " autofocus \>
          <label for="tables">Comanda</label>
      </div>
    </div>
    <div class="modal-footer">
        <button class="modal-close waves-effect waves-green btn-flat" id="clientcancel">Cancelar</button>
        <button class="modal-close waves-effect waves-green btn-flat" id="clientok">OK</button>
    </div>
  </div>
  <script>
      const clientCancel=document.getElementById('clientcancel');
     

      document.getElementById('clientinput').focus()

       clientCancel.addEventListener('click',()=>{
          const clientInput=document.getElementById('clientinput');
         // console.log(tablesinput)
          clientInput.value=null;
         
          
        })



    </script>