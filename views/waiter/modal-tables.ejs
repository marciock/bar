 <!-- Modal Structure -->
 <div id="modaltables" class="modal modal-fixed-footer">
    <div class="modal-content">
      <h4>Mesa</h4>
      <p>Escolha a mesa</p>

      <div class="input-field col s6 m4">
          <input placeholder="Mesa" id="tablesinput" type="number" class="validate" autofocus \>
          <label for="tables">Mesa</label>
      </div>

      <div id="result-table"></div>
    </div>
    <div class="modal-footer">
     <button class="modal-close waves-effect waves-green btn-flat" id="tablescancel">Cancelar</button>
     <button class="modal-close waves-effect waves-green btn-flat" id="tableok">OK</button>
    </div>
  </div>
<script>
  const tablesCancel=document.getElementById('tablescancel');
   tablesCancel.addEventListener('click',()=>{
      const tablesinput=document.getElementById('tablesinput');
     // console.log(tablesinput)
      tablesinput.value=null;
      
    })
</script>
  