 <!-- Modal Structure -->
 <div id="modal-tables" class="modal modal-fixed-footer">
    <div class="modal-content">
      <h4 id="title-table">Mesa</h4>
      <p id="coment-table">Selecione o código da Mesa</p>
      <div id="content-table">
        
      </div>
      <div class="row">
        <div class="input-field col s6">
          <input type="number" id="code-table" name="code">
          <label for="code-table" class="active">Código</label>
        </div>
      </div>
    </div>
    <div class="modal-footer">
        <button class="modal-close waves-effect waves-green btn-flat" id="table-cancel">Cancelar</button>
        <button class="modal-close waves-effect waves-green btn-flat" id="table-ok">OK</button>
       
    </div>
  </div>
  